{
  "name": "@sylphlab/tools-json",
  "version": "0.6.0",
  "description": "Core logic for MCP JSON tools",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run"
  },
  "keywords": [
    "mcp",
    "core",
    "json",
    "encode",
    "decode",
    "diff",
    "patch"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sylphlab/tools-core": "workspace:*",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^3.1.2"
  }
}
