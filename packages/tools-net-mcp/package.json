{
  "name": "@sylphlab/tools-net-mcp",
  "version": "0.4.4",
  "description": "MCP server providing network tools",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "bin": {
    "mcp-net": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch"
  },
  "keywords": [
    "mcp",
    "tool",
    "net",
    "network"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.17.30",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.10.1",
    "@sylphlab/tools-adaptor-mcp": "workspace:*",
    "@sylphlab/tools-core": "workspace:*",
    "@sylphlab/tools-net": "workspace:*",
    "zod": "^3.23.8"
  }
}
