{
  "name": "@sylphlab/mcp-hasher-core",
  "version": "0.5.2",
  "description": "Core logic for MCP hashing tools",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": ["mcp", "core", "hash", "hasher", "md5", "sha256", "sha512"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sylphlab/mcp-core": "workspace:*",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@vitest/coverage-v8": "^3.1.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^3.1.1"
  }
}
