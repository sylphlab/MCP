{
  "name": "@sylphlab/mcp-wait",
  "version": "0.3.5",
  "description": "MCP server providing wait tool",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "bin": {
    "mcp-wait": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch"
  },
  "keywords": ["mcp", "tool", "wait", "delay"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.12.12",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.10.1",
    "@sylphlab/mcp-core": "workspace:*",
    "@sylphlab/mcp-wait-core": "workspace:*",
    "zod": "^3.23.8",
    "@sylphlab/mcp-utils": "workspace:*"
  }
}
